<van-tabs active="{{ active }}" bind:change="onChange">
  <van-tab title="缺勤名单">
		<van-cell wx:for="{{absenceList}}">{{item.studentId}}{{item.studentName}}</van-cell>
	</van-tab>
  <van-tab title="请假名单">
		<van-cell wx:for="{{leaveList}}">{{item.studentId}}{{item.studentName}}</van-cell>
	</van-tab>
  <van-tab title="迟到名单">
		<van-cell wx:for="{{lateList}}">{{item.studentId}}{{item.studentName}}</van-cell>
	</van-tab>
</van-tabs>


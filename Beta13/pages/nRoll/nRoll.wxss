page{
  height: 100%;
}

.video_box{width: 100%;height: auto;position: fixed;top:0;bottom: 0;background-color: rgb(216, 198, 198);}
.video_list{width: 100%;height: 100vh;position: relative;}
.video_list video{position: absolute;top:50%;margin-top:-30vw; width: 100%;height:56vw;padding: 0;}

.ani-box{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  transform: translateY(0px);
  z-index: 0;
}

.swipe-cell{
  margin-top: 15px;
}

.list1{
  height: 100%;
	font-size: 100rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  line-height: 300rpx;
}

.cell-view{
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
}

.cell-container{
  border-radius: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* height: 1300rpx; */
  background-color: white;
  width: 700rpx;
}

.cell{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 100rpx;
}

.label{
  display: flex;
  justify-content: center;
	font-size: large;
  height: 100%;
  width: 375rpx;

}

.left{
  display: flex;
  align-items: center;
  font-size: 100rpx;
  background-color: red;
  color: white;
  width: 660rpx;
  height: 100%;
  border-radius: 50rpx;
}

.left-text{
  margin-left: 230rpx;
}

.right{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 100rpx;
  background-color: green;
  color: white;
  width: 660rpx;
  height: 100%;
  border-radius: 50rpx;
}

.right-text{
  margin-left: 230rpx;
}
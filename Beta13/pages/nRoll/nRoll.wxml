<view class="video_box">
	<view bindtouchend="touchEnd" id="{{index}}" animation="{{animation}}" bindtouchstart="touchStart" bindtouchmove="touchMove" class="video_list" wx:for="{{video_list}}" data-index="{{index}}" wx:key="index" >
		<view class="swipe-cell ani-box" bindtouchstart="startEvent" bindtouchend="endEvent">
			<van-swipe-cell left-width="{{360}}" right-width="{{ 360 }}">
				<view slot="left" class="left">
					<view class="left-text">未到</view>
				</view>
				<view class="cell-view">
					<view class="cell-container" style="height:{{windowHeight-30}}px;">
						<view class="cell">
							<text>000030000</text>
							<text>{{item.value}}</text>
							<text>hhh</text>
						</view>
					</view>
				</view>
				<view slot="right" class="right">
					<view class="right-text">到课</view>
				</view>
			</van-swipe-cell>
		</view>
	</view>
</view>